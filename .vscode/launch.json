{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "STM32F103ZET Launch",
            "type": "cppdbg",
            "request": "launch",
            "targetArchitecture": "arm",//虽然官方说弃用了，但实际上必须指明
            "program": "${workspaceFolder}/build/STM32F103ZET.elf", // 采用了自定义的设置，这里没啥用了
            "args": [""],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "D:/App/env/tools/gnu_gcc/arm_gcc/mingw/bin/arm-none-eabi-gdb.exe",
            "setupCommands": [
                {
                    "text": "-enable-pretty-printing",
                    "description": "Enable pretty-printing for gdb",
                    "ignoreFailures": true
                }
            ],
            "launchCompleteCommand": "None",
            "miDebuggerServerAddress": "localhost:2331",
            "debugServerArgs": "-se",
            "customLaunchSetupCommands": [
                {
                    "text": "target remote :2331",
                    "description": "connect to server",
                    "ignoreFailures": false
                },
                {
                    "text": "file E:/docunment/1/transplant_LiteOS/transplant/STM32F103ZET/build/STM32F103ZET.elf",
                    "description": "load file to gdb",
                    "ignoreFailures": false
                },
                {
                    "text": "load",
                    "description": "download file to MCU",
                    "ignoreFailures": false
                },
                {
                    "text": "monitor reset",
                    "description": "reset MCU",
                    "ignoreFailures": false
                },
                {
                    "text": "b MX_GPIO_Init",
                    "description": "set breakpoints at MX_GPIO_Init",
                    "ignoreFailures": false
                }
            ]
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "jlink",
            "cwd": "${workspaceRoot}",
            "executable": "E:/docunment/1/transplant_LiteOS/transplant/STM32F103ZET/build/STM32F103ZET.elf",
            "name": "Debug (J-Link)",
            "device": "STM32F103ZE",
            "interface": "jtag",
            "ipAddress": null,
            "serialNumber": null,
        }
    ]
}